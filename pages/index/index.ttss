.history-section {
    margin: 100rpx auto 20rpx auto;
    width: 90%;
    overflow-x: hidden;
}

.history-header {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 40rpx;
}

.history-title {
    background: url(https://cdn.juesedao.cn/mdy/f5f8f5f3a77d4bd79d8472fdd1e50b77) no-repeat;
    background-size: 100% 100%;
    font-size: 40rpx;
    font-weight: 600;
    color: #02092F;
    margin-bottom: 0;
    display: flex;
    align-items: center;
    justify-content: center;
}

.core-advantage-section {
    width: 90%;
    margin: 100rpx auto 0 auto;
    flex-direction: column;
    align-items: center;
}

.core-advantage-title {
    font-size: 40rpx;
    font-weight: 700;
    color: #1822a0;
    margin-bottom: 40rpx;
    letter-spacing: 2rpx;
}

.core-advantage-row {
    width: 100%;
    max-width: 700rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    margin: 0 auto;
    box-sizing: border-box;
    margin-top: 22px;
}

.core-advantage-item {
    width: 46%;
    min-width: 140rpx;
    max-width: 320rpx;
    display: flex;
    flex-direction: column;
    align-items: flex-start;
    background: transparent;
    margin-bottom: 24rpx;
}

.core-advantage-icon {
    width: 40rpx;
    height: 40rpx;
}

.core-advantage-label {
    color: #02092F;
    font-family: "Alibaba PuHuiTi 3.0";
    font-size: 16px;
    font-style: normal;
    font-weight: bold;
    margin-left: 12rpx;
}

.core-advantage-desc {
    text-align: left;
    color: #4E536E;
    font-family: "Alibaba PuHuiTi 3.0";
    font-size: 12px;
    font-style: normal;
    font-weight: 400;
    line-height: 140%;
    /* 16.8px */
    text-transform: uppercase;
    margin-top: 10px;
}

.core-advantage-desc-1 {
    color: #4E536E;
    margin-top: 10px;
    text-align: right;
    font-family: "Alibaba PuHuiTi 3.0";
    font-size: 12px;
    font-style: normal;
    font-weight: 400;
    line-height: 140%;
    /* 16.8px */
    text-transform: uppercase;
}

.core-advantage-center-img {
    width: 260px;
    height: 128px;
    display: block;
    margin: 32rpx auto 32rpx auto;
}

@media screen and (max-width: 500px) {

    .core-advantage-item {
        width: 48%;
