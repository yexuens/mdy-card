}

.news .item image {
    position: relative;
    display: block;
}

.news .item .name {
    position: absolute;
    bottom: 0;
    left: 0;
    font-size: 24rpx;
    opacity: 0.7;
    background: #00000090;
    color: rgba(255, 255, 255, 1);
    width: 94%;
    backdrop-filter: blur(80rpx);
    padding: 20rpx;
    border-radius: 0px 0px 30rpx 30rpx;
    white-space: nowrap;
    /* 禁止换行 */
    overflow: hidden;
    /* 隐藏溢出 */
    text-overflow: ellipsis;
    /* 显示省略号 */
}

