page {
    /* background: #ecf5fe; */
    /* overflow: hidden; */
    height: 100%;
    background: url(https://cdn.juesedao.cn/mdy/7cb96f27b416481684af7accb44a4113) no-repeat;
    background-size: cover;
    overflow-x: hidden;
}

.history-scroll {
    width: 100vw;
    white-space: nowrap;
    overflow-x: scroll;
    position: relative;
}

.marquee-track {
    display: flex;
    width: max-content;
    animation: marquee-move 20s linear infinite;
    animation-play-state: running;
}

.marquee-track.paused {
    animation-play-state: paused;
}

@keyframes marquee-move {
    0% {
        transform: translateX(0);
    }

    100% {
        transform: translateX(-50%);
    }
}

.history-card {
    display: inline-block;
    vertical-align: top;
    width: 260rpx;
    border: 1px solid #fff;
    background: linear-gradient(229deg, rgba(255, 255, 255, 0.00) 3.29%, rgba(255, 255, 255, 0.80) 86.36%);
    border-radius: 20rpx;
    box-shadow: 0 2rpx 12rpx 0 rgba(0, 0, 0, 0.06);
    margin-right: 24rpx;
    padding: 32rpx 24rpx 24rpx 24rpx;
    text-align: left;
}

.history-year {
    font-size: 32rpx;
    font-weight: 700;
    color: #2173fd;
    margin-bottom: 12rpx;
}

.history-desc {
    font-size: 26rpx;
    color: #333;
    line-height: 1.5;
}

.user {
    width: 90%;
    margin: 0 auto;
    border-radius: 0 0 40rpx 40rpx;
    overflow: hidden;
}

.user-top-section {
